<h1>Add Doctor</h1>
<%= form_with(model: @doctor, local: true, class: "ui form") do |f| %>
  <div class="field">
    <%= f.label :last_name %>
    <%= f.text_field :last_name, required: true, autofocus: true, placeholder: "Last Name" %>
  </div>

  <%= f.submit nil, class: "ui green button inverted" %>
<% end %>